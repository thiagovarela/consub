{
	"useTabs": true,
	"singleQuote": true,
	"tabWidth": 4,
	"semi": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"svelteSortOrder": "options-scripts-styles-markup",
	"htmlWhitespaceSensitivity": "ignore",
	"bracketSameLine": true
}
